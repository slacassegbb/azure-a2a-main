    },
    "blinking_white": {
      "meaning": "On and looking for internet connectivity",
      "status": "connecting",
      "action": "Wait 2-5 minutes for connection to establish"
    },
    "blinking_yellow": {
      "meaning": "Modem is turning on",
      "status": "booting",
      "action": "Wait for modem to fully boot up (3-5 minutes)"
    },
    "solid_yellow": {
      "meaning": "On but connectivity issue",
      "status": "degraded",
      "action": "Check backend configuration and signal strength"
    },
    "red": {
      "meaning": "Internal issue with modem",
      "status": "error",
      "action": "Modem may need replacement or advanced troubleshooting"
    },
    "no_light": {
      "meaning": "Modem is off or has no power",
      "status": "offline",
      "action": "Check power connection, power cycle modem"
    }
