# Azure Voice Live API Configuration
# Get your token by running: az account get-access-token --scope https://ai.azure.com/.default --query accessToken -o tsv
NEXT_PUBLIC_AZURE_AI_TOKEN=your-azure-ai-token-here

# Azure AI Foundry Project Configuration
NEXT_PUBLIC_AZURE_AI_FOUNDRY_PROJECT_ENDPOINT=https://owenv-foundry-resource.services.ai.azure.com/api/projects/owenv-foundry
NEXT_PUBLIC_VOICE_MODEL=gpt-4o-realtime

# Backend API Configuration
NEXT_PUBLIC_A2A_API_URL=http://localhost:12000
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:12000/events

# Debug logging
NEXT_PUBLIC_DEBUG_LOGS=true
