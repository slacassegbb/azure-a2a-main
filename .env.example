# Core API Keys & Secrets
# Replace with real keys/secrets when deploying.
SECRET_KEY="change-me"
JWT_ALGORITHM="HS256"

# Azure AI Foundry & Model Configuration
# Required for AI Foundry-hosted agents.
AZURE_AI_FOUNDRY_PROJECT_ENDPOINT="
AZURE_AI_AGENT_MODEL_DEPLOYMENT_NAME="gpt-4o"
AZURE_OPENAI_GPT_API_BASE=""
AZURE_OPENAI_GPT_API_KEY=""
AZURE_OPENAI_GPT_API_VERSION=""
AZURE_OPENAI_GPT_DEPLOYMENT="gpt-4o"

# Azure OpenAI Embeddings (used for memory/search - optional)
AZURE_OPENAI_EMBEDDINGS_ENDPOINT=""
AZURE_OPENAI_EMBEDDINGS_DEPLOYMENT="text-embedding-3-large"
AZURE_OPENAI_EMBEDDINGS_KEY=""

# Azure Search (used for memory/search - optional)
AZURE_SEARCH_SERVICE_ENDPOINT=""
AZURE_SEARCH_ADMIN_KEY=""
AZURE_SEARCH_INDEX_NAME="microsoft-results"
AZURE_SEARCH_VECTOR_DIMENSION="3072"

# Azure Blon Storage (used for clooud file storage - optional)
AZURE_STORAGE_CONNECTION_STRING=""
AZURE_STORAGE_ACCOUNT_NAME="yourstorageaccount"
AZURE_BLOB_CONTAINER="a2a-files"
AZURE_BLOB_SIZE_THRESHOLD="1048576"
FORCE_AZURE_BLOB="false"

# Azure Content Understanding (used for document processing - optional)
AZURE_CONTENT_UNDERSTANDING_ENDPOINT=""
AZURE_CONTENT_UNDERSTANDING_API_VERSION="2024-12-01-preview"

# Backend Server Configuration
A2A_HOST="FOUNDRY"
A2A_UI_HOST="0.0.0.0"
A2A_UI_PORT="12000"
BACKEND_SERVER_URL="http://localhost:12000"
WEBSOCKET_SERVER_URL="http://localhost:8080"
GOOGLE_GENAI_USE_VERTEXAI="FALSE"

# Frontend (Next.js) Public Config
NEXT_PUBLIC_A2A_API_URL="http://localhost:12000"
NEXT_PUBLIC_DEV_MODE="true"
NEXT_PUBLIC_DEBUG_LOGS="false"
NEXT_PUBLIC_WEBSOCKET_URL="ws://localhost:8080/events"
NEXT_PUBLIC_WEBSOCKET_MAX_INITIAL_ATTEMPTS="3"

#Telemetry (used for azure ai foundry Agent tracing - optional)
APPLICATIONINSIGHTS_CONNECTION_STRING=""

#Used for google remote agent - not needed
GOOGLE_API_KEY=""