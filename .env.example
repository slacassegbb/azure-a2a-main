# Core API Keys & Secrets
# Replace with real keys/secrets when deploying.
SECRET_KEY="change-me"
JWT_ALGORITHM="HS256"

# Required for AI Foundry-hosted agents.
AZURE_AI_FOUNDRY_PROJECT_ENDPOINT="" #This is the only important key, found in the azure AI foundry Portal overview under Azure AI Foundry project endpoint
AZURE_AI_AGENT_MODEL_DEPLOYMENT_NAME="gpt-4o" #must have a model deployed, found in Azure AI Portal under 'Models + endpoints'
AZURE_OPENAI_GPT_API_BASE="" #found in azure AI foundry portal under Azure AI Services endpoint
AZURE_OPENAI_GPT_API_KEY="" #found in the azure AI foundry Portal overview under API Key
AZURE_OPENAI_GPT_API_VERSION="" #must match model
AZURE_OPENAI_GPT_DEPLOYMENT="gpt-4o" #must have a model deployed, found in Azure AI Portal under 'Models + endpoints'

# Azure OpenAI Embeddings (used for memory/search - optional)
AZURE_OPENAI_EMBEDDINGS_ENDPOINT="" #must have an embeddings model for vector memory, endpoint can be found in azure AI foundry, under 'models + Endpoints", model details
AZURE_OPENAI_EMBEDDINGS_DEPLOYMENT="text-embedding-3-large" #model name must be exactly like embedding model name in azure ai foundry under models + endpoints
AZURE_OPENAI_EMBEDDINGS_KEY="" #API Key can be found in azure AI foundry, under 'models + Endpoints", embeddings model details

# Azure Search (used for memory/search - optional)
AZURE_SEARCH_SERVICE_ENDPOINT="" #found in the azure search service portal in overview under Url
AZURE_SEARCH_ADMIN_KEY="" #found in the azure search service portal under settings, keys, primary admin key
AZURE_SEARCH_INDEX_NAME="microsoft-results"
AZURE_SEARCH_VECTOR_DIMENSION="3072"

# Azure Blob Storage (used for cloud file storage - optional)
# Option 1: Managed Identity (Recommended - more secure, no keys to manage)
# Comment out AZURE_STORAGE_CONNECTION_STRING and only use AZURE_STORAGE_ACCOUNT_NAME
# Requires: Azure role "Storage Blob Data Contributor" assigned to your user/managed identity
AZURE_STORAGE_ACCOUNT_NAME="yourstorageaccount" #your storage account name in the azure portal

# Option 2: Connection String (Legacy - requires key-based authentication enabled on storage account)
# Uncomment the line below if your storage account allows key-based authentication
# AZURE_STORAGE_CONNECTION_STRING="DefaultEndpointsProtocol=https;AccountName=yourstorageaccount;AccountKey=YOUR_ACCOUNT_KEY;EndpointSuffix=core.windows.net"

AZURE_BLOB_CONTAINER="a2a-files" #name it what you want
AZURE_BLOB_SIZE_THRESHOLD="1048576"
FORCE_AZURE_BLOB="true" #set to false if you do not want to use cloud storage

# Azure Content Understanding (used for document processing - optional)
AZURE_CONTENT_UNDERSTANDING_ENDPOINT="" #found in the azure AI foundry portal under Content Understadning Overview
AZURE_CONTENT_UNDERSTANDING_API_VERSION="2024-12-01-preview" #found in the azure AI foundry portal under Content Understadning Overview

# Backend Server Configuration
A2A_HOST="FOUNDRY"
A2A_UI_HOST="0.0.0.0"
A2A_UI_PORT="12000"
BACKEND_SERVER_URL="http://localhost:12000"
WEBSOCKET_SERVER_URL="http://localhost:8080"
GOOGLE_GENAI_USE_VERTEXAI="FALSE"

# Frontend (Next.js) Public Config
NEXT_PUBLIC_A2A_API_URL="http://localhost:12000"
NEXT_PUBLIC_DEV_MODE="true"
NEXT_PUBLIC_DEBUG_LOGS="false"
NEXT_PUBLIC_WEBSOCKET_URL="ws://localhost:8080/events"
NEXT_PUBLIC_WEBSOCKET_MAX_INITIAL_ATTEMPTS="3"

#Telemetry (used for azure ai foundry Agent tracing - optional)
APPLICATIONINSIGHTS_CONNECTION_STRING="" #can be found in azure portal application insights under configure, properties, CONNECTION STRING

#Used for google remote agent - not needed
GOOGLE_API_KEY=""